<?php

namespace Studmine\StatsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Honnor
 *
 * @ORM\Table(name="honor")
 * @ORM\Entity(repositoryClass="Studmine\StatsBundle\Repository\HonorRepository")
 */
class Honor
{
    /**
     * @var integer
     *
     * @ORM\Column(name="honor_id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer")
     */
    private $value;

    /**
     * @ORM\ManyToOne(targetEntity="Studmine\StatsBundle\Entity\User")
     * @ORM\JoinColumn(nullable=false, referencedColumnName="user_id", name="receiver_user_id")
     */
    private $receiverUser;

    /**
     * @ORM\ManyToOne(targetEntity="Studmine\StatsBundle\Entity\User")
     * @ORM\JoinColumn(nullable=false, referencedColumnName="user_id", name="donor_user_id")
     */
    private $donorUser;

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set value
     *
     * @param integer $value
     * @return Honnor
     */
    public function setValue($value)
    {
        $this->value = $value;
    
        return $this;
    }

    /**
     * Get value
     *
     * @return integer 
     */
    public function getValue()
    {
        return $this->value;
    }

    /**
     * Set receiverUser
     *
     * @param \Studmine\StatsBundle\Entity\User $receiverUser
     * @return Honnor
     */
    public function setReceiverUser(\Studmine\StatsBundle\Entity\User $receiverUser)
    {
        $this->receiverUser = $receiverUser;
    
        return $this;
    }

    /**
     * Get receiverUser
     *
     * @return \Studmine\StatsBundle\Entity\User 
     */
    public function getReceiverUser()
    {
        return $this->receiverUser;
    }

    /**
     * Set donorUser
     *
     * @param \Studmine\StatsBundle\Entity\User $donorUser
     * @return Honnor
     */
    public function setDonorUser(\Studmine\StatsBundle\Entity\User $donorUser)
    {
        $this->donorUser = $donorUser;
    
        return $this;
    }

    /**
     * Get donorUser
     *
     * @return \Studmine\StatsBundle\Entity\User 
     */
    public function getDonorUser()
    {
        return $this->donorUser;
    }
}
