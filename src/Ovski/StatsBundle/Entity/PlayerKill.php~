<?php

namespace Studmine\StatsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PlayerKill
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="Studmine\StatsBundle\epository\PlayerKillRepository")
 */
class PlayerKill
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer")
     */
    private $weapon;

    /**
     * @var \DateTime
     *
     * @ORM\Column(type="datetime")
     */
    private $date;

    /**
     * @ORM\ManyToOne(targetEntity="Studmine\StatsBundle\Entity\Player")
     * @ORM\JoinColumn(nullable=false)
     */
    private $player_killed;

    /**
     * @ORM\ManyToOne(targetEntity="Studmine\StatsBundle\Entity\Player")
     * @ORM\JoinColumn(nullable=false)
     */
    private $player_killer;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set weapon
     *
     * @param integer $weapon
     * @return PlayerKill
     */
    public function setWeapon($weapon)
    {
        $this->weapon = $weapon;
    
        return $this;
    }

    /**
     * Get weapon
     *
     * @return integer 
     */
    public function getWeapon()
    {
        return $this->weapon;
    }

    /**
     * Set date
     *
     * @param \DateTime $date
     * @return PlayerKill
     */
    public function setDate($date)
    {
        $this->date = $date;
    
        return $this;
    }

    /**
     * Get date
     *
     * @return \DateTime 
     */
    public function getDate()
    {
        return $this->date;
    }

    /**
     * Set player_killed
     *
     * @param \Studmine\StatsBundle\Entity\Player $playerKilled
     * @return PlayerKill
     */
    public function setPlayerKilled(\Studmine\StatsBundle\Entity\Player $playerKilled)
    {
        $this->player_killed = $playerKilled;
    
        return $this;
    }

    /**
     * Get player_killed
     *
     * @return \Studmine\StatsBundle\Entity\Player 
     */
    public function getPlayerKilled()
    {
        return $this->player_killed;
    }

    /**
     * Set player_killer
     *
     * @param \Studmine\StatsBundle\Entity\Player $playerKiller
     * @return PlayerKill
     */
    public function setPlayerKiller(\Studmine\StatsBundle\Entity\Player $playerKiller)
    {
        $this->player_killer = $playerKiller;
    
        return $this;
    }

    /**
     * Get player_killer
     *
     * @return \Studmine\StatsBundle\Entity\Player 
     */
    public function getPlayerKiller()
    {
        return $this->player_killer;
    }
}
