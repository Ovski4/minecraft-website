<?php

namespace Studmine\StatsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PlayerStats
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="Studmine\StatsBundle\Entity\PlayerStatsRepository")
 */
class PlayerStats
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer")
     */
    private $blocksBroken;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer")
     */
    private $blocksBroken;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer")
     */
    private $blocksPlaced;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer")
     */
    private $stupidDeaths;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer")
     */
    private $deaths;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer")
     */
    private $timePlayed;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer")
     */
    private $vebosity;

    /**
     * @ORM\OneToOne(targetEntity="Studmine\StatsBundle\Entity\Player")
     * @ORM\JoinColumn(nullable=false)
     */
    private $player;

}
