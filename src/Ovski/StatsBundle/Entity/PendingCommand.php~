<?php

namespace Studmine\StatsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PendingCommand
 *
 * @ORM\Table(name="pending_command")
 * @ORM\Entity(repositoryClass="Studmine\StatsBundle\Repository\PendingCommandRepository")
 */
class PendingCommand
{
    /**
     * @var integer
     *
     * @ORM\Column(type="integer", name="pending_command_id")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $pendingCommandId;

    /**
     * @ORM\ManyToOne(targetEntity="Studmine\StatsBundle\Entity\User")
     * @ORM\JoinColumn(nullable=false, referencedColumnName="user_id", name="expected_user_id")
     */
    private $expectedUser;

    /**
     * @var string
     *
     * @ORM\Column(type="string", length="255", "name="remaining_time")
     */
    private $command;

    /**
     * @var datetime
     *
     * @ORM\Column(type="datetime", name="created_at")
     */
    private $createdAt;

    /**
     * @var datetime
     *
     * @ORM\Column(type="datetime", name="executed_at")
     */
    private $executedAt;

}
