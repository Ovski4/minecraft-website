<?php

namespace Studmine\StatsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * UserKill
 *
 * @ORM\Table(name="kill")
 * @ORM\Entity(repositoryClass="Studmine\StatsBundle\Repository\UserKillRepository")
 */
class UserKill
{
    /**
     * @var integer
     *
     * @ORM\Column(name="kill_id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(type="integer", name="weapon_id")
     */
    private $weapon;

    /**
     * @var \DateTime
     *
     * @ORM\Column(type="datetime")
     */
    private $date;

    /**
     * @ORM\ManyToOne(targetEntity="Studmine\StatsBundle\Entity\User")
     * @ORM\JoinColumn(nullable=false, referencedColumnName="user_id", name="killed_user_id")
     */
    private $killedUser;

    /**
     * @ORM\ManyToOne(targetEntity="Studmine\StatsBundle\Entity\User")
     * @ORM\JoinColumn(nullable=false, referencedColumnName="user_id", name="killer_user_id")
     */
    private $killerUser;

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set weapon
     *
     * @param integer $weapon
     * @return UserKill
     */
    public function setWeapon($weapon)
    {
        $this->weapon = $weapon;
    
        return $this;
    }

    /**
     * Get weapon
     *
     * @return integer 
     */
    public function getWeapon()
    {
        return $this->weapon;
    }

    /**
     * Set date
     *
     * @param \DateTime $date
     * @return UserKill
     */
    public function setDate($date)
    {
        $this->date = $date;
    
        return $this;
    }

    /**
     * Get date
     *
     * @return \DateTime 
     */
    public function getDate()
    {
        return $this->date;
    }

    /**
     * Set killedUser
     *
     * @param \Studmine\StatsBundle\Entity\User $killedUser
     * @return UserKill
     */
    public function setKilledUser(\Studmine\StatsBundle\Entity\User $killedUser)
    {
        $this->killedUser = $killedUser;
    
        return $this;
    }

    /**
     * Get killedUser
     *
     * @return \Studmine\StatsBundle\Entity\User 
     */
    public function getKilledUser()
    {
        return $this->killedUser;
    }

    /**
     * Set killerUser
     *
     * @param \Studmine\StatsBundle\Entity\User $killerUser
     * @return UserKill
     */
    public function setKillerUser(\Studmine\StatsBundle\Entity\User $killerUser)
    {
        $this->killerUser = $killerUser;
    
        return $this;
    }

    /**
     * Get killerUser
     *
     * @return \Studmine\StatsBundle\Entity\User 
     */
    public function getKillerUser()
    {
        return $this->killerUser;
    }
}
