<?php

namespace Studmine\StatsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Jail
 *
 * @ORM\Table(name="jail")
 * @ORM\Entity(repositoryClass="Studmine\StatsBundle\Repository\JailRepository")
 */
class Jail
{
    /**
     * @var integer
     *
     * @ORM\Column(type="integer", name="jail_id")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $jail_id;

    /**
     * @var long
     *
     * @ORM\Column(type="bigint", length=255)
     */
    private $x;

    /**
     * @var long
     *
     * @ORM\Column(type="bigint", length=255)
     */
    private $y;

    /**
     * @var long
     *
     * @ORM\Column(type="bigint", length=255)
     */
    private $z;
}
